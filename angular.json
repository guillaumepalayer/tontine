{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Betoken": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/Betoken",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/scss/_user.scss",
              "src/assets/scss/_user-variables.scss",
              "src/assets/fonts/feather/feather.css"
            ],
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/chart.js/dist/Chart.min.js",
              "node_modules/list.js/dist/list.min.js",
              "src/assets/libs/chart.js/Chart.extension.js",
              "src/assets/js/polyfills.js",
              "src/assets/js/charts.js",
              "src/assets/js/autosize.js",
              "src/assets/js/dashkit.js",
              "src/assets/js/dropdowns.js",
              "src/assets/js/flatpickr.js",
              "src/assets/js/navbar.js",
              "src/assets/js/popover.js",
              "src/assets/js/select2.js",
              "src/assets/js/tooltip.js",
              "src/assets/js/demo.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "zh-Hans": {
              "aot": true,
              "outputPath": "dist/Betoken/zh-Hans",
              "i18nFile": "src/locale/messages.zh-Hans.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "zh-Hans",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "ja": {
              "aot": true,
              "outputPath": "dist/Betoken/ja",
              "i18nFile": "src/locale/messages.ja.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "ja",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "pt": {
              "aot": true,
              "outputPath": "dist/Betoken/pt",
              "i18nFile": "src/locale/messages.pt.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "pt",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "Betoken:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Betoken:build:production"
            },
            "zh-Hans": {
              "browserTarget": "Betoken:build:zh-Hans"
            },
            "ja": {
              "browserTarget": "Betoken:build:ja"
            },
            "pt": {
              "browserTarget": "Betoken:build:pt"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Betoken:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/assets/scss/theme.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-builders/custom-webpack:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    },
    "Betoken-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-builders/custom-webpack:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Betoken:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Betoken:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-builders/custom-webpack:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "Betoken"
}